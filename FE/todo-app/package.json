    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@ngrx/signals": "^20.0.1",
    "@ngrx/store": "^20.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.2",
    "@angular/cli": "^20.3.2",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.12",
    "jasmine-core": "~5.9.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.2.5",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
